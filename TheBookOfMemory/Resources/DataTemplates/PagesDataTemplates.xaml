<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pages="clr-namespace:TheBookOfMemory.ViewModels.Pages"
                    xmlns:pages1="clr-namespace:TheBookOfMemory.Views.Pages">
    <DataTemplate DataType="{x:Type pages:MainPageViewModel}">
        <pages1:MainView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:EventPageViewModel}">
        <pages1:EventPage DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:SelectHeroPageViewModel}">
        <pages1:SelectHeroView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type pages:PersonalInformationViewModel}">
        <pages1:PersonalInformationPage DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>