<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:TheBookOfMemory.ViewModels.Popups"
                    xmlns:popups1="clr-namespace:TheBookOfMemory.Views.Popups">
    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type popups:InactivityPopupViewModel}">
        <popups1:InactivityPopup DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type popups:FilterPopupViewModel}">
        <popups1:FilterPopup DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type popups:HistoryPopupViewModel}">
        <popups1:HistoryPopup DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>